<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <filter>
        <filter-name>filter</filter-name>
        <filter-class>filter.filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>controller.home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>managerAccount</servlet-name>
        <servlet-class>adminController.managerAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>manaOrdering</servlet-name>
        <servlet-class>adminController.manaOrdering</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>process_comment</servlet-name>
        <servlet-class>controller.process_comment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>managerAccount</servlet-name>
        <url-pattern>/managerAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>manaOrdering</servlet-name>
        <url-pattern>/manaOrdering</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>process_comment</servlet-name>
        <url-pattern>/process_comment</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/home</location>
    </error-page>
</web-app>
